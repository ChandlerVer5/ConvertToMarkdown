{
  "name": "@gxl/epub-parser",
  "version": "0.4.3",
  "description": "A simple epub parser",
  "main": "build/lib/epubParser.js",
  "types": "build/types/index.d.ts",
  "scripts": {
    "build-old": "npm run build:lib && npm run build:es && npm run build:umd && npm run build:umd:min",
    "build:js": "tsc --outDir js --declaration --declarationDir types",
    "prebuild:lib": "npm run build:js",
    "build:lib": "babel js --out-dir lib",
    "prebuild:es": "npm run build:js",
    "build:es": "cross-env BABEL_ENV=es babel js --out-dir es",
    "build:umd": "cross-env NODE_ENV=development webpack src/index.ts dist/simple-epub-parser.js",
    "build:umd:min": "cross-env NODE_ENV=production webpack src/index.ts dist/simple-epub-parser.min.js",
    "clean": "rimraf dist lib js es types",
    "watch:lib-old": "npm run build:js -- --watch & npm run build:lib -- --watch",
    "preupdate-package": "npm run clean && npm run build:lib && npm config set registry https://registry.npmjs.org/",
    "update-package": "npm publish",
    "postupdate-package": "npm config set registry https://registry.npm.taobao.org",
    "build": "webpack --config config/webpack.config.js --progress",
    "watch:lib": "npm run build -- --watch",
    "debug": "nodemon --debug=9229 --inspect src/test.js",
    "test": "jest --no-cache",
    "watch:test": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gaoxiaoliangz/epub-parser.git"
  },
  "keywords": [
    "epub-parser",
    "parser",
    "epub",
    "simple",
    "easy",
    "book",
    "file"
  ],
  "author": "gaoxiaoliangz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gaoxiaoliangz/simple-epub-parser/issues"
  },
  "homepage": "https://github.com/gaoxiaoliangz/simple-epub-parser#readme",
  "dependencies": {
    "@types/md5": "^2.1.32",
    "jsdom": "9.12.0",
    "lodash": "^4.17.2",
    "md5": "^2.2.1",
    "node-zip": "^1.1.1",
    "to-markdown": "^3.0.3",
    "typescript": "^2.3.3",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "@types/expect.js": "^0.3.28",
    "@types/express": "^4.0.35",
    "@types/lodash": "^4.14.43",
    "@types/mocha": "^2.2.33",
    "@types/node": "0.0.2",
    "@types/xml2js": "0.0.32",
    "app-root-path": "^2.0.1",
    "awesome-typescript-loader": "^3.1.3",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^2.0.3",
    "babel-plugin-lodash": "^3.1.2",
    "babel-plugin-syntax-async-functions": "^6.5.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-regenerator": "^6.6.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-no-commonjs": "0.0.2",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "coveralls": "^2.11.15",
    "cross-env": "^3.1.3",
    "dotenv": "^4.0.0",
    "expect": "^1.14.0",
    "expect.js": "^0.3.1",
    "express": "^4.15.3",
    "jest": "18.1.0",
    "lodash-webpack-plugin": "^0.10.3",
    "mocha": "^3.0.2",
    "mocha-lcov-reporter": "^1.2.0",
    "promise": "^7.1.1",
    "rifraf": "^2.0.3",
    "rimraf": "^2.5.1",
    "source-map-support": "^0.4.15",
    "ts-jest": "^20.0.4",
    "ts-node": "^3.0.4",
    "tslint": "4.5.1",
    "vrsource-tslint-rules": "^5.1.0",
    "webpack": "^2.6.0",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__test__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
